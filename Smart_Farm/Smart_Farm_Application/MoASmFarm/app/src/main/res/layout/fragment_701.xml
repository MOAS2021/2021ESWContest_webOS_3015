<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sub7fragment.Fragment701">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/seed1"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginStart="52dp"
                android:layout_marginTop="200dp"
                android:src="@drawable/seedsangchu"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/seed2"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginTop="200dp"
                android:layout_marginEnd="52dp"
                android:src="@drawable/seedleaf"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/seed3"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginStart="52dp"
                android:layout_marginTop="80dp"
                android:src="@drawable/seedbaj"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seed1" />

            <ImageView
                android:id="@+id/seed4"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginTop="80dp"
                android:layout_marginEnd="52dp"
                android:src="@drawable/seedbuch"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seed2" />

            <ImageView
                android:id="@+id/seed5"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginStart="52dp"
                android:layout_marginTop="80dp"
                android:src="@drawable/seedsigum"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seed3" />

            <ImageView
                android:id="@+id/seed6"
                android:layout_width="130dp"
                android:layout_height="200dp"
                android:layout_marginTop="80dp"
                android:layout_marginEnd="52dp"
                android:src="@drawable/seedchung"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seed4" />

            <Button
                android:id="@+id/btn1"
                android:layout_width="350dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="1150dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.491"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="70dp"
                android:text="모아씨앗"
                android:textSize="32sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.core.widget.NestedScrollView>